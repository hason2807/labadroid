    <script>

        // 登录弹窗错误提示
        function toggleErr(that, text) {
            var fn = text === '' ? 'hide' : 'show'
            var fn2 = text === '' ? 'removeClass' : 'addClass'

            that.parents('.base-public-rlg-group')[fn2]('group--error').find('.base-public-rlg-group-error').text(text)[fn]()
        }

        let identify_alert = '' ? true : false
        // 弹出授权许可基础信息form
        function showLicenseeModal(){
            if(identify_alert){
                $('.window_pop_bg').css({display: 'block'})
                $('.licensee-pop').show()
                identify_alert = false
                $.ajax({
                    type: 'POST',
                    url: '/?m=Home&c=Detail&a=userIdentifyInfoClose',
                    dataType: 'json',
                    success: function (res) {}
                })
                return true
            }
            return false
        }

        function closeLicenseeModal(){
            $('.licensee-pop,.window_pop_bg').fadeOut(300)
        }

        // 授权许可基础信息信息form
        function licenseeFormAction(){

            $('.identity-type .base-public-rlg-group-input').on('click', function(event){
                event.stopPropagation();
                event.preventDefault()
                $('#identity-type-opts').show()
            })

            $(document).on('click',function(){
                $('#identity-type-opts').hide()
            })

            $('#identity-type-opts .label').on('click', function(){
                const label = $(this).html();
                const value = $(this).data('value')
                $('#identity_type_label').html(label);
                $('#base-public-identity-type').val(value)
                if(value=='1'){
                    $('#base-public-licensee-address').hide()
                    $('#base-public-licensee-enterprise-code').hide()

                    $('#base-public-licensee-name').attr('placeholder', $('#base-public-licensee-name').attr('placeholder-person'))
                    $('#base-public-licensee-email').attr('placeholder', $('#base-public-licensee-email').attr('placeholder-person'))

                }else{
                    $('#base-public-licensee-address').show()
                    $('#base-public-licensee-enterprise-code').show()
                    $('#base-public-licensee-name').attr('placeholder', $('#base-public-licensee-name').attr('placeholder-company'))
                    $('#base-public-licensee-email').attr('placeholder', $('#base-public-licensee-email').attr('placeholder-company'))
                }
            })

            $('#base-public-rlg-licensee-btn').on('click', function(){
                let isVerified = true
                const  identity_type = $('#base-public-identity-type').val()// 企业 ｜ 个人
                const licensee_email_address = $('#base-public-licensee-email').val(); // 邮箱
                const licensee_name = $('#base-public-licensee-name').val() // 被许可人
                const licensee_address = $('#base-public-licensee-address').val() // 企业地址
                const licensee_enterprise_code = $('#base-public-licensee-enterprise-code').val() // 企业代码

                let params = {
                    identity_type,
                    licensee_email_address,
                    licensee_name
                }

                if(identity_type){
                    toggleErr($('#base-public-identity-type'), "")
                    isVerified=true
                }else{
                    isVerified=false
                    toggleErr($('#base-public-identity-type'), $('#base-public-identity-type').data('error'))
                }
                if(licensee_name){
                    toggleErr($('#base-public-licensee-name'), "")
                    isVerified=true
                }else{
                    isVerified=false
                    toggleErr($('#base-public-licensee-name'), $('#base-public-licensee-name').data('error'))
                }

                if(licensee_email_address){
                    const emailRegExp = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
                    if(emailRegExp.test(licensee_email_address)){
                        toggleErr($('#base-public-licensee-email'), "")
                        isVerified=true
                    }else{


                        toggleErr($('#base-public-licensee-email'), "Vui lòng kiểm tra định dạng email của bạn")
                        isVerified=false
                    }
                }else{
                    isVerified=false
                    toggleErr($('#base-public-licensee-email'), $('#base-public-licensee-email').data('error'))
                }

                if(identity_type!='1'){
                    Object.assign(params, { licensee_address, licensee_enterprise_code })
                    if(licensee_address){
                        toggleErr($('#base-public-licensee-address'), "")
                        isVerified=true
                        if(licensee_address.length<5){
                            isVerified=false
                            toggleErr($('#base-public-licensee-address'), 'The input address is too short')
                        }
                    }else{
                        isVerified=false
                        toggleErr($('#base-public-licensee-address'), $('#base-public-licensee-address').data('error'))
                    }
                }

                if(isVerified){
                    $.ajax({
                        type: 'POST',
                        url: '/?m=Home&c=Detail&a=userIdentifyInfo',
                        dataType: 'json',
                        data: params,
                        success: function (res) {
                            if(res.status==1){
                                closeLicenseeModal()
                            }else{
                                alert(res.msg)
                            }
                        }
                    })
                }
            })
        }

        licenseeFormAction()
    </script>
