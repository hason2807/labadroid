        scrollBind() {
            $(document).scroll(function () {
                const scroll0 = $('body').scrollTop() + $('html').scrollTop();
                let peiZhi = $(".peizhi_index");
                if (scroll0 > 200) {
                    peiZhi.addClass('fixed-header').css({"top": "0", "opacity": "1"});
                } else {
                    peiZhi.removeClass('fixed-header').css({"top": "-80px", "opacity": "0"});
                }

            });

            $(document).scroll(function () {
                var scroll0 = $('body').scrollTop() + $('html').scrollTop();
                if (scroll0 > 200) {
                    $(".anniversary_head_two").addClass("fixed-bottom");
                } else {
                    $(".select_all_pop").hide();
                    $(".head_search").blur();
                    $(".anniversary_head_two").removeClass("fixed-bottom");
                }
            });
        },
        deleteCookie: () => {
            if (ld.extensionInstalled) {
                const love_deals = $('html').attr('love-deals');
                if (!love_deals) {
                    $.ajax({
                        type: "get",
                        url: "/deal/api/v1/display/",
                    })
                }
            }
        }
    };
</script>

    <script>
        $(document).ready(function () {
            ld.extentionInstallDisplay();
            ld.utmBind();
        });
    </script>
</body>
