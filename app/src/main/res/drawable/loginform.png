    <script type="text/javascript" data-base-js>
    var ld = {
        getAllInfo: 0,
        extensionUrl: 'https://chromewebstore.google.com/detail/lovedeals-automatic-coupo/ocallppfcfgngjhoplfhlmekcdjaeapl',
        extensionInstalled: 0,
        openExtentionTab: (url) => {
            const {innerHeight, innerWidth} = window;
            const w = innerWidth * 0.7;
            const h = innerHeight * 0.7;
            const l = (innerWidth - w) / 2;
            const t = (innerHeight - h) / 2;
            window.open(url ? url : ld.extensionUrl, 'couponPlugin', `width=${w},height=${h},top=${t},left=${l}`)
        },
        //插件显示
        checkVisible(id, element) {
            if (element.is(':visible')) {
                ld.extensionStatic([id]);
            } else {
                setTimeout(function () {
                    ld.checkVisible(id, element);
                }, 1000);
            }
        },
        //插件展示统计
        extensionStatic: (extentionTypes) => {
            extentionTypes = extentionTypes ? extentionTypes : [];
            if (extentionTypes.length === 0) {
                $('button[data-extention],a[data-extention]').each(function () {
                    let extId = parseInt($(this).data('extention'));
                    if (extId === 5 && !$('#download_popup').is(':visible')) {
                        ld.checkVisible(extId, $('#download_popup'));
                        return true;
                    }
                    if (extId && (!$(this).parent().is(':visible') || !$(this).parent().parent().is(':visible') || !$(this).parent().parent().parent().is(':visible'))) {
                        let element = $(this).parent();
                        if (!$(this).parent().parent().is(':visible')) {
                            element = $(this).parent().parent();
                        } else if (!$(this).parent().parent().parent().is(':visible')) {
                            element = $(this).parent().parent().parent();
                        }
                        ld.checkVisible(extId, element);
                        return true;
                    }
                    $(this).data('extention') && extentionTypes.push($(this).data('extention'))
                });
            }

            if (extentionTypes.length > 0) {
                $.ajax({
                    type: 'post',
                    data: {
                        type: extentionTypes
                    },
                    url: 'https://ajax.lovepik.com/?c=extention&a=view',
                    success: function (d) {
                    }
                });
            }
        },
        utmBind: () => {
            ld.extensionStatic();
            $('a[data-utm]').on('click', function (event) {
                event.preventDefault();
                ld.openExtentionTab($(this).data('utm'));
            });
        },
        openInstallModal: () => {
            if (!ld.extensionInstalled && window.innerWidth >= 1024 && $('.plugin-install-modal').length > 0) {
                const delayShowPlugin = setTimeout(() => {
                    $('.plugin-install-modal').addClass('show')
                }, 30000);

                $(document).on('click', function () {
                    clearTimeout(delayShowPlugin)
                })
            }
        },
        getDongTaiInfo: () => {
            if (!ld.getAllInfo) {
                return !1;
            }
            $.ajax({
                type: 'post',
                url: env['site'] + '/dongtai/getAllInfo',
                success: function (d) {
                    if (d.status === true && d.html) {
                        is_ajax = true;
                        $(".information-list").html(d.html);
                        $(".information-block").css({
                            opacity: 1,
                            visibility: 'visible',
                            top: '62px'
                        })
                    }
                }
            });
        },
        openNotice: () => {
            if (!ld.isFacebookShare) {
                return !1;
            }
            $(window).load(function () {
                env.doAlert('/download/showMsg', {
                    'title': 'Notice',
                    'notice': 'Your have got 2 free download ! (including 1 Powerpoint)',
                    'btn': 'I know',
                    'js': ";location.href=location.href.replace(/share=\\d+/,'')"
                });
            });
        },
        //优惠券谷歌插件安装推广
        extentionInstallDisplay: () => {
            const plugin_install_close = localStorage.getItem("plugin_install") ?? '';
            const plugin_close_time = new Date(Number(plugin_install_close));
            const isTimeout = plugin_close_time.setHours(plugin_close_time.getHours() + 3) < new Date();

            if (isTimeout) {
                $('.coupons-bottom-fixed').css('display', 'block');
            }

            $('.coupons-bottom-fixed .act-close').on('click', function (e) {
                $('.coupons-bottom-fixed').css('display', 'none');
                localStorage.setItem("plugin_install", new Date().getTime());
            });
        },
        addGoogleGsi: () => {
            if (!ld.isNotRobotAndMobile) {
                return !0;
            }
            $('<script>')
                .attr('src', 'https://accounts.google.com/gsi/client')
                .attr('async', '')
                .attr('defer', '')
                .appendTo('body');
        },
        scrollBind() {
            $(document).scroll(function () {
                const scroll0 = $('body').scrollTop() + $('html').scrollTop();
                let peiZhi = $(".peizhi_index");
                if (scroll0 > 200) {
                    peiZhi.addClass('fixed-header').css({"top": "0", "opacity": "1"});
                } else {
                    peiZhi.removeClass('fixed-header').css({"top": "-80px", "opacity": "0"});
                }

            });

            $(document).scroll(function () {
                var scroll0 = $('body').scrollTop() + $('html').scrollTop();
                if (scroll0 > 200) {
                    $(".anniversary_head_two").addClass("fixed-bottom");
                } else {
                    $(".select_all_pop").hide();
                    $(".head_search").blur();
                    $(".anniversary_head_two").removeClass("fixed-bottom");
                }
            });
        },
        deleteCookie: () => {
            if (ld.extensionInstalled) {
                const love_deals = $('html').attr('love-deals');
                if (!love_deals) {
                    $.ajax({
                        type: "get",
                        url: "/deal/api/v1/display/",
                    })
                }
            }
        }
    };
</script>

