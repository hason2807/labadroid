                                        }
                                    } catch (ex) {
                                        // NS_ERROR_DOM_SECURITY_ERR
                                        if (ex.code === 1000) {
                                            isLoaded = true;
                                        }
                                    }
                                }
                                if (isLoaded) {
                                    setTimeout(function () {
                                        callback(null, node);
                                    }, 1);
                                } else {
                                    setTimeout(function () {
                                        poll(node, callback);
                                    }, 10);
